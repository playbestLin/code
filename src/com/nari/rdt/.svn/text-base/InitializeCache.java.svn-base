package com.nari.rdt;

import java.util.*;

import com.nari.rdt.util.SystemUtil;

public class InitializeCache {
	
	private static List<String> HostAddress = SystemUtil.getHosts();
	
	public static List<String> getHostAddress() {
		return HostAddress;
	}
	
	public static List<String> getHostAddressAgain() {
		HostAddress = SystemUtil.getHosts();
		return HostAddress;
	}
}
